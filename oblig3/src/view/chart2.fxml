<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.chart2Controller">
    <VBox prefWidth="1500.0">
        <children>
            <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" style="&#10;" text="Chart2" textAlignment="CENTER">
                <font>
                    <Font size="18.0" fx:id="x1" />
                </font>
                <textFill>
                    <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
                </textFill>
            </Label>
        </children>
    </VBox>
   <BarChart fx:id="barChart" layoutX="325.0" prefHeight="720.0" prefWidth="850.0">
     <xAxis>
       <CategoryAxis fx:id="xAxis" side="BOTTOM" />
     </xAxis>
     <yAxis>
       <NumberAxis fx:id="yAxis" side="LEFT" />
     </yAxis>
   </BarChart>
</AnchorPane>
